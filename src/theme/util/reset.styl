* {
  outline: none;
  -webkit-tap-highlight-color: rgba(0,0,0,0);
  -webkit-tap-highlight-color: transparent;
}


a, a:hover, a:visited, a:active {
  color: inherit;
  text-decoration: inherit;
}


table, tr, td, th, tbody, thead, tfoot {
  border-collapse: collapse
  border-spacing: 0
  padding: 0
  margin: 0
  text-align: left
  vertical-align: top
}
