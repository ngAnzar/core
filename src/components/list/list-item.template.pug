:stylus
  @require "./list"

  .content
    display: flex
    align-items: center
    justify-content: space-between
    white-space: nowrap

  .ripple
    position: absolute
    top: 0
    right: 0
    bottom: 0
    left: 0
    overflow: hidden
    pointer-events: none

ng-content
div.ripple(#ripple)

