

svg(
  [style.width]="width",
  [style.height]="height",
  [style.transform]="'rotate(-90deg)'",
  [attr.width]="width",
  [attr.height]="height",
  preserveAspectRatio="xMidYMid meet",
  focusable="false")
  circle(
    #circle,
    [attr.cx]="radius",
    [attr.cy]="radius",
    [attr.r]="_radiusMinusStroke",
    [attr.stroke-width]="strokeWidth",
    [attr.stroke-dasharray]="dashArray",
    fill="none",
    stroke-linecap="round")
